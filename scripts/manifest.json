{
  "manifest_version": 2,

  "name": "Project Yolo",
  "description": "This extension shows ayyy lmao",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon64.png",
    "default_popup": "popup.html",
    "default_title": "Click me!"
  },

  "permissions": ["activeTab", "http://*/", "https://*/"],

  "content_security_policy": "script-src 'self' 'unsafe-eval' https://js.arcgis.com; object-src 'self'"
}